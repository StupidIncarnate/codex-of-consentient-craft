{
  "attribution": {
    "commit": "Claude'd it up in here!",
    "pr": ""
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "npx tsx $CLAUDE_PROJECT_DIR/packages/hooks/src/startup/start-session-start-hook.ts"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "npx tsx $CLAUDE_PROJECT_DIR/packages/hooks/src/startup/start-pre-edit-hook.ts"
          }
        ]
      }
    ],
    "PostToolUse": []
  },
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(chmod:*)",
      "Bash(rg:*)",
      "Bash(grep:*)",
      "Bash(npm install:*)",
      "Bash(npm test)",
      "Bash(npm:*)",
      "Bash(rm:*)",
      "Bash(node:*)",
      "Bash(npm test:*)",
      "Bash(ls:*)",
      "Write",
      "Bash(find:*)",
      "Bash(npm run:*)",
      "mcp__ide__getDiagnostics",
      "Bash(tree:*)",
      "WebFetch(domain:*)",
      "Bash(env)",
      "Bash(claude config:*)",
      "Bash(npx tsc:*)",
      "Bash(npx ts-node:*)",
      "Bash(npx eslint:*)",
      "Bash(tee:*)",
      "Bash(echo:*)",
      "Bash(npm ls:*)",
      "Bash(npx @modelcontextprotocol/inspector:*)",
      "Bash(npx --version)",
      "Bash(npx:*)",
      "WebSearch",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:www.npmjs.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "Bash(npm link)",
      "Bash(npm view:*)",
      "Bash(npm show:*)",
      "WebFetch(domain:registry.npmjs.org)"
    ],
    "deny": ["Bash(cat:*)", "Bash(sed:*)"]
  }
}