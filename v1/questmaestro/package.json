{
  "name": "questmaestro",
  "version": "0.1.0-beta.1",
  "description": "A fun, quest-driven AI orchestration system for Claude that makes context windows work better",
  "bin": {
    "questmaestro": "./dist/bin/cli.js",
    "questmaestro-setup": "./dist/bin/install.js"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc && cp -r ../../v1/commands dist/ && chmod +x dist/bin/cli.js && chmod +x dist/bin/install.js",
    "build:clean": "rm -rf dist && npm run build",
    "test": "jest",
    "test:unit": "jest",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/questmaestro.git"
  },
  "keywords": [
    "claude",
    "ai",
    "orchestration",
    "context-management",
    "quest",
    "slash-commands"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist/",
    "src/",
    "bin/",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.0",
    "@questmaestro/hooks": "^0.1.0",
    "@questmaestro/standards": "^0.1.0",
    "@types/ioredis": "^4.28.10",
    "ioredis": "^5.7.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}