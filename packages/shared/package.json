{
  "name": "@questmaestro/shared",
  "version": "0.1.0",
  "description": "Shared contracts and utilities for Questmaestro packages",
  "exports": {
    "./contracts": {
      "import": "./dist/contracts.js",
      "require": "./dist/contracts.js",
      "types": "./dist/contracts.d.ts"
    },
    "./guards": {
      "import": "./dist/guards.js",
      "require": "./dist/guards.js",
      "types": "./dist/guards.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint"
  },
  "keywords": [
    "questmaestro",
    "contracts",
    "zod",
    "validation",
    "typescript"
  ],
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@questmaestro/testing": "*",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
