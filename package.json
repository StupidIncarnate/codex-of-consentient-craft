{
  "name": "questmaestro",
  "version": "0.1.0",
  "description": "A fun, quest-driven AI orchestration system for Claude that makes context windows work better",
  "bin": {
    "questmaestro": "./bin/install.js"
  },
  "scripts": {
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:watch": "jest --watch --testPathIgnorePatterns=e2e",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=e2e",
    "test:e2e": "RUN_E2E_TESTS=true jest tests/e2e",
    "test:all": "npm test && npm run test:e2e",
    "claude": "claude",
    "claude:version": "claude --version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/questmaestro.git"
  },
  "keywords": [
    "claude",
    "ai",
    "orchestration",
    "context-management",
    "quest",
    "slash-commands"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.4"
  }
}
