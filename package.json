{
  "name": "questmaestro",
  "version": "0.1.0-beta.1",
  "description": "A fun, quest-driven AI orchestration system for Claude that makes context windows work better",
  "bin": {
    "questmaestro": "./dist/bin/install.js"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest bin src tests/utils",
    "test:e2e": "RUN_E2E_TESTS=true jest tests/e2e",
    "test:all": "npm test && npm run test:e2e",
    "lint": "eslint tests/**/*.ts bin/**/*.ts --fix",
    "lint:check": "eslint tests/**/*.ts bin/**/*.ts",
    "typecheck": "tsc --noEmit",
    "claude": "claude",
    "claude:version": "claude --version",
    "spike": "./spike/claude-interactive/joke-file-watcher.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/questmaestro.git"
  },
  "keywords": [
    "claude",
    "ai",
    "orchestration",
    "context-management",
    "quest",
    "slash-commands"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
